  <% unless @user == current_user %>
    <% if @friend_if %>
      <%= link_to "Unfriend dog", @friend_if, method: :delete, class: 'btn btn-danger'  %>
    <% else %>
      <%= link_to "Friend dog", friendships_path(friend_id: @user), method: :post, class: 'btn btn-primary'%>
    <% end %>
  <% end %>